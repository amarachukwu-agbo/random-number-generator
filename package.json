{
  "name": "random-number-generator",
  "version": "1.0.0",
  "description": "A simple web application that generates phone numbers for a telephone company",
  "main": "server/index.js",
  "scripts": {
    "test": "jest -u --forceExit --env=jsdom",
    "server:start-dev": "nodemon --exec babel-node server/index.js",
    "server:build": "babel server --out-dir lib",
    "start": "node lib/index.js",
    "client:start-dev": "parcel ./client/index.html",
    "client:build": "parcel build ./client/index.html",
    "lint": "eslint --ext .jsx,.js server/ client/",
    "heroku-postbuild": "yarn run server:build && yarn run client:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amarachukwu-agbo/random-number-generator.git"
  },
  "keywords": [
    "React",
    "Node",
    "Express"
  ],
  "author": "Agbo Amarachukwu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amarachukwu-agbo/random-number-generator/issues"
  },
  "homepage": "https://github.com/amarachukwu-agbo/random-number-generator#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.0.0-0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "nodemon": "^1.18.11",
    "parcel-bundler": "^1.12.3",
    "react-testing-library": "^6.1.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "file-saver": "^2.0.1",
    "node-sass": "^4.11.0",
    "prop": "^0.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "toastr": "^2.1.4"
  },
  "engines": {
    "node": "11.x"
  }
}
